{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "*": ["*"]
    },
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,

    "outDir": "./build",
    "rootDir": "./src"
  },
  "include": ["src"]
}

